{
  "name": "@hasezoey/verify-typegoose-issues",
  "version": "0.0.1",
  "description": "Bootstrap TS Project All Files needed for TS",
  "main": "lib/app.js",
  "type": "module",
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "scripts": {
    "start": "npm run build && npm run run",
    "run:directly": "ts-node ./src/test.ts",
    "run": "npm run build && node lib/test.js",
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "doc": "typedoc --out ./doc ./src --mode modules",
    "clean": "rm -rf ./doc ./lib ./coverage ./nyc_output",
    "prepare": "npm run build"
  },
  "repository": "none",
  "author": "hasezoey",
  "license": "MIT",
  "devDependencies": {
    "@types/mongoose": "^5.7.34",
    "@types/node": "^14.0.27",
    "@types/source-map-support": "^0.5.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@typegoose/auto-increment": "^0.5.0",
    "@typegoose/typegoose": "^7.3.1",
    "mongoose": "^5.9.26",
    "source-map-support": "^0.5.19"
  }
}
